  "definitions": {
    "fillKey": {
      "type": "string",
      "title": "The fillKey schema",
      "description": "Primary represent official language, secondary represent common language .",
      "default": ""
    },
    "Name": {
      "type": "string",
      "title": "The Name schema",
      "description": "Name of the country",
      "default": ""
    },
    "Country_flag": {
      "type": "string",
      "title": "The Country_flag schema",
      "description": "the file name in flag folder which to show the country flag",
      "default": ""
    },
    "languages": {
      "type": "string",
      "title": "The languages schema",
      "description": "Information about the spoken languages in the country",
      "default": ""
    },
    "culture": {
      "type": "string",
      "title": "The culture schema",
      "description": "Information about the culture"
    },
    "Audio": {
      "type": "array",
      "title": "The Audio schema",
      "description": "A list of audio files to demonstrate the different dialects",
      "default": [],
      "example": [
        [
          "Titel (optional)",
          "audiofile.mp3"
        ]
      ],
      "additionalItems": true,
      "items": {
        "anyOf": [
          {
            "type": "array",
            "additionalItems": false,
            "items": [
              {
                "type": "string"
              },
              {
                "type": "string"
              }
            ]
          }
        ]
      }
    },
    "Video": {
      "type": "array",
      "title": "The Video schema",
      "description": "A list of video files (in images directory) or shorted youtube links",
      "default": [],
      "example": [
        [
          "Video Title",
          "Youtube link in this style !!!! -> https://youtu.be/57waq1HdllQ"
        ],
        [
          "Title",
          "VideoFile.mp4"
        ]
      ],
      "additionalItems": true,
      "items": {
        "anyOf": [
          {
            "type": "array",
            "additionalItems": false,
            "maxItems": 2,
            "items": [
              {
                "type": "string"
              },
              {
                "type": "string"
              }
            ]
          }
        ]
      }
    },
    "Image": {
      "type": "array",
      "title": "The Image schema",
      "description": "List of images (path of file in image folder) shown in gallery.",
      "default": [],
      "example": [
        "Image01.jpg",
        "Image02.jpg"
      ],
      "additionalItems": true,
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "Description": {
      "type": "string",
      "title": "The Description schema",
      "description": "Information about the country as plain text or HTML",
      "default": ""
    },
    "Famous_people": {
      "type": "string",
      "title": "The Famous_people schema",
      "description": "Information about famous peoples in the country",
      "default": ""
    }
  }
